<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Seminarios" table="seminarios">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visible" type="boolean" column="visible" nullable="true"/>
    <field name="summary" type="text" column="summary" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="content" type="text" column="content" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="source" type="string" column="source" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdate" type="datetime" column="createdate" nullable="false"/>
    <field name="editdate" type="datetime" column="editdate" nullable="false"/>
    <field name="hasimage" type="boolean" column="hasimage" nullable="false"/>
    <field name="imagesource" type="string" column="imagesource" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hasdocument" type="boolean" column="hasdocument" nullable="false"/>
    <field name="documentsource" type="string" column="documentsource" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aforo" type="string" column="aforo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="preinscripcion" type="string" column="preinscripcion" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
