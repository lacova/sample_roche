<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FosUser" table="fos_user">
    <unique-constraints>
      <unique-constraint name="UNIQ_957A647992FC23A8" columns="username_canonical"/>
      <unique-constraint name="UNIQ_957A6479A0D96FBF" columns="email_canonical"/>
      <unique-constraint name="UNIQ_957A6479C05FB297" columns="confirmation_token"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fax" type="string" column="fax" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phoneext" type="string" column="phoneext" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="center" type="string" column="center" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="facultad" type="string" column="facultad" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="departamento" type="string" column="departamento" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ciudad" type="string" column="ciudad" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliente1" column="cliente1" type="integer" >
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliente2" column="cliente2" type="integer" >
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliente3" column="cliente3" type="integer" >
      <options>
        <option name="fixed"/>
      </options>
    </field>    
  </entity>
</doctrine-mapping>
