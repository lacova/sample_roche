<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Cliente" table="cliente">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codigosap" type="string" column="codigosap" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="facultad" type="string" column="facultad" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="departamento" type="string" column="departamento" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ciudad" type="string" column="ciudad" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaalta" column="fechaalta" type="datetime" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eshospital" column="eshospital" type="boolean" >
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clientede" column="clientede" type="string" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>    
    <field name="status" column="status" type="boolean" >
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
